<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="serviciopersonas.model.Persona" />
	<class name="serviciopersonas.model.impl.PersonaImpl" table="FOO_Persona">
		<id name="dni" type="java.lang.String">
			<generator class="assigned" />
		</id>
		<property name="nombre" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="edad" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
</hibernate-mapping>